<div class="container">

  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">
      <p class="fc-username left">
        <% if current_user.type ==  'Client'%>
          <%= link_to User.find(params[:user_id]).name, user_path(params[:user_id]) %>さんの予定
        <% end %>
      </p>
      <div id="calendar"></div>
    </div>
  </div>
</div>

